{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ApplicationInsights": {
    "ConnectionString": "InstrumentationKey=YOUR_ACTUAL_APPLICATION_INSIGHTS_KEY"
  },
  "PerformanceSettings": {
    "EnableSlowEndpoints": true,
    "ResponseTimeThresholdMs": 500
  }
}